<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="bo.com.kibo.entidades.Destino" table="t11_destino">
        <id name="id" type="java.lang.Integer">
            <column name="dest_id" />
            <generator class="identity" />
        </id>
        <property name="nombre" type="string">
            <column name="dest_nombre" length="50" not-null="true" unique="true" />
        </property>
        <property name="modificado" type="timestamp" update="false" insert="false">
            <column name="modificado" length="19" />
        </property>
    </class>
</hibernate-mapping>
